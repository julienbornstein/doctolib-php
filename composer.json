{
    "name": "julienbornstein/doctolib-php",
    "description": "Doctolib PHP Client",
    "type": "library",
    "keywords": [
        "doctolib",
        "package"
    ],
    "homepage": "https://github.com/julienbornstein/doctolib-php",
    "license": "MIT",
    "authors": [
        {
            "name": ":Julien Bornstein",
            "email": "julien@parisbouge.com",
            "homepage": "https://www.parisbouge.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/http-client": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/options-resolver": "^5.2",
        "symfony/serializer-pack": "^1.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpmd/phpmd": "^2.10",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/phpstan": "^0.12.87",
        "phpunit/phpunit": "9.*",
        "symfony/debug-pack": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Doctolib\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Doctolib\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
